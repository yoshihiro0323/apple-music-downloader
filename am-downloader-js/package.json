{
  "name": "apple-music-downloader",
  "version": "1.0.0",
  "description": "Apple Music URLからアルバム、曲、プレイリスト情報を取得するNode.jsライブラリ",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "lint": "eslint src/**/*.js",
    "start": "node src/index.js",
    "example": "node examples/example.js",
    "dev": "nodemon examples/example.js",
    "build": "babel src -d dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "apple-music",
    "downloader",
    "music",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "m3u8-parser": "^7.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "sinon": "^17.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "commonjs"
}
